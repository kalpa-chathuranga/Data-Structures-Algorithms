package BinaryTreePackage;

public class BinaryTree {

	private Node root;
	
	public BinaryTree() {
		root = null;
	}
	
	void addNode(int value)
	{
		if( root == null)
		{
			root= new Node(value);
		}
		
		if(value < root.data)
		{
			root.left = new Node(value);
		}
		else
		{
			root.right = new Node(value);
		}
		
		
	}
	
}


