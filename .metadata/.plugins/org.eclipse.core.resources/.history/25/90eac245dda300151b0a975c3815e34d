
public class MS {

	
	
	public void mergeSort(int[] a)
	{
		int n = a.length;
		
		// Base case for partitioning
		// Array with single element is already sorted
		if(n==1) return;
		
		int middle = n/2;
		// Create sub arrays
		int[] left = new int[middle];
		int[] right = new int[n-middle];
		
		
		
	}
}
